filter_non_numeric([], []).  % ??????? ??????: ?????? ??????

filter_non_numeric([H|T], [H|Result]) :-
    \+ number(H),  % ???? H ?? ?????, ????????? ??? ? ?????????
    filter_non_numeric(T, Result).

filter_non_numeric([H|T], Result) :-
    number(H),  % ???? H ?????, ?????????? ???
    filter_non_numeric(T, Result).

?- filter_non_numeric([1, 2, a, 3, b, 4.5, c], Result).